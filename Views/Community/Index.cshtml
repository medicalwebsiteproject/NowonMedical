@{
}
<h1 class="community-title">@ViewBag.Title</h1>
<table class="table table-striped table-hover">
	<thead>
		<tr>
			<th scope="col">글번호</th>
			<th scope="col">제목</th>
			<th scope="col">게시일</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var board in ViewBag.Boards)
		{
			<tr>
				<td>@board.BoardId</td>
				<td>@board.Title</td>
				<td>@board.Created</td>
			</tr>
		}
	</tbody>
</table>